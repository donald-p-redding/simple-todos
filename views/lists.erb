<ul id="lists">
  <% sort_lists(@lists) do |list| %>
    <li class="<%= list_class(list) %>">
      <a href="/lists/<%= list[:id] %>">
        <h2><%= list[:name] %></h2>
        <p><%= list[:incomplete]%>  / <%= list[:total] %></p>
      </a>
    </li>
  <% end %>
</ul>

<% content_for :header_links do %>
  <a class="add" href="/lists/new" id="#">New List</a>
<% end %>
